<?xml version="1.0"?>
<root>
  <list>
    <item>
      <name>[Key Repeat] Initial Wait</name>
      <sysctl>keyremap4macbook.repeat.initial_wait</sysctl>
      <default>500</default>
      <baseunit>ms</baseunit>
    </item>
    <item>
      <name>[Key Repeat] Wait</name>
      <sysctl>keyremap4macbook.repeat.wait</sysctl>
      <default>30</default>
      <baseunit>ms</baseunit>
    </item>
    <item>
      <name>[Pointing Device] CursorMove to ScrollWheel Threshold</name>
      <sysctl>keyremap4macbook.pointing.relative2scroll_threshold</sysctl>
      <default>16</default>
      <baseunit>ms</baseunit>
    </item>
  </list>
</root>
