        <appendix>Use 7 to VK_CONSUMERKEY_MUSIC_NEXT</appendix>
        <appendix>Use 8 to VK_CONSUMERKEY_VOLUME_MUTE</appendix>
        <appendix>Use 9 to VK_CONSUMERKEY_VOLUME_DOWN</appendix>
