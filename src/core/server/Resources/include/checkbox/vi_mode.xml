      <autogen>
        __KeyToKey__
        KeyCode::H, ModifierFlag::FN,
        KeyCode::CURSOR_LEFT,
        DelayUntilRepeat::RawValue::0, KeyRepeat::RawValue::0
      </autogen>
      <autogen>
        __KeyToKey__
        KeyCode::J, ModifierFlag::FN,
